{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["svelte3"],
  "overrides": [
    {
      "files": ["**/*.svelte"],
      "processor": "svelte3/svelte3",
      "rules": {
        "quotes": ["warn", "single"]
      }
    }
  ],
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["warn", "single"]
  }
}
